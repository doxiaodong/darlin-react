{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "experimentalDecorators": true,
    "baseUrl": "./src",
    "lib": [
      "es2017",
      "dom"
    ],
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}
